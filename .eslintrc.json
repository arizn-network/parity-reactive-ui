{
	"extends": ["semistandard", "standard-react"],
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"mocha": true,
		"node": true
	},
	"globals": {
	},
	"rules": {
		"indent": ["error", "tab"],
		"no-tabs": 0,
		"react/jsx-indent": ["error", "tab"],
		"react/jsx-indent-props": ["error", "tab"]
	}
}
